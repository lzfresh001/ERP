public class WorkBookDetail {

	private Integer wbdId;        // 数据字典系(细)表id
	private Integer wid;          // 数据字典系(细)外键
	private String wbdName;       // 数据字典系(细)表名称
	private String remark;        // 数据字典系(细)表备注
	private Integer delflag;      // 数据字典系(细)表删除标记  1 表示未删除   0 表示已删除
	private String currentTime;   // 数据字典系(细)表创建时间
	private String updateTime;    // 数据字典系(细)表更新时间
	private String optionPerson;  // 数据字典系(细)表操作人
	public Integer getWbdId() {
		return wbdId;
	}
	public void setWbdId(Integer wbdId) {
		this.wbdId = wbdId;
	}
	public Integer getWid() {
		return wid;
	}
	public void setWid(Integer wid) {
		this.wid = wid;
	}
	public String getWbdName() {
		return wbdName;
	}
	public void setWbdName(String wbdName) {
		this.wbdName = wbdName;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Integer getDelflag() {
		return delflag;
	}
	public void setDelflag(Integer delflag) {
		this.delflag = delflag;
	}
	public String getCurrentTime() {
		return currentTime;
	}
	public void setCurrentTime(String currentTime) {
		this.currentTime = currentTime;
	}
	public String getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}
	public String getOptionPerson() {
		return optionPerson;
	}
	public void setOptionPerson(String optionPerson) {
		this.optionPerson = optionPerson;
	}
	
}
